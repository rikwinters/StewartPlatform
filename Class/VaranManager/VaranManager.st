//[<retval]ISO start point [ns]
FUNCTION VIRTUAL GLOBAL VaranManager::GetIsoStartPoint
VAR_OUTPUT
  retval	  	: UDINT;
END_VAR

  //if init is not executed yet
  if ( s_VaranManagerSettings.us_InitCounter = 0 ) then
    GetISOStartTime();
  end_if;	 

  retval := VaranManagerTime * IsoStartPoint / 100;

