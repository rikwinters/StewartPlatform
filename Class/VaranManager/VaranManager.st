//[#ENGLISH]
//=================================================================================================
//Function to log faults in cyclic (to prevent an error 67)
//=================================================================================================
//[server]	State					:W		:State of class
//[members]	s_VaranManagerSettings	:W		:structure for different varan settings
//			p_KernelLog				:R		:pointer to kernel log function
FUNCTION  VaranManager::LogError
VAR
  ud_reason	      : UDINT;
  b_SetError    	: BOOL;
END_VAR

  ud_reason := 0;

  b_SetError := FALSE;

  //look which entry
  case ( State ) of 	
    
    _TimeSliceError:	//Log entry
              if ( p_KernelLog <> NIL ) then
                OS_KernelLog1( "VaranManager %d: Timeslice Error! Manager is set off!", VaranManagerNr$DINT );
              end_if;
              ud_reason := VM_ERROR_TIMESLICE;
              b_SetError := TRUE;

    _TimesliceErrorIRQ:	//Log entry
              if ( p_KernelLog <> NIL ) then
                OS_KernelLog1( "VaranManager %d: Timeslice Error Interrupt, Manager is set off!", VaranManagerNr$DINT );
              end_if;
              ud_reason := VM_ERROR_TIMESLICE_IRQ;
              b_SetError := TRUE;

    _WatchdogError:		//Log entry
              if ( p_KernelLog <> NIL ) then
                OS_KernelLog1( "VaranManager %d: Watchdog Error Interrupt, Manager is set off!", VaranManagerNr$DINT );
              end_if;
              ud_reason := VM_ERROR_WATCHDOG;
              b_SetError := TRUE;

    _RequiredError:		//Log entry
              if ( p_KernelLog <> NIL ) then
                OS_KernelLog1( "VaranManager %d: Required Module not ready, Manager is set off!", VaranManagerNr$DINT );
              end_if;
              //b_SetError := FALSE;

    _DeviceError:		//Log entry
              if ( p_KernelLog <> NIL ) then
                OS_KernelLog1( "VaranManager %d: Error on Device, Manager is set off!", VaranManagerNr$DINT );
              end_if;
              ud_reason := VM_ERROR_DEVICE;
              b_SetError := TRUE;
              
    _CDIASRequiredError, _DIASRequiredError, _DiasError: //Log Entry
              if ( p_KernelLog <> NIL ) then
                OS_KernelLog1( "VaranManager %d: Required DIAS/CDIAS Module not ready, Manager is set off!", VaranManagerNr$DINT );
              end_if;
              ud_reason := VM_ERROR_DEVICE;
              b_SetError := TRUE;
              State := _RequiredError;
    
     else     //Unknown fault, log that an error occured
              if ( p_KernelLog <> NIL ) then
                OS_KernelLog2( "VaranManager %d: Unknown Fault (State = %d), Manager is set off!", VaranManagerNr$DINT, State$DINT);
              end_if;
  end_case;

  //reset log flag
  s_VaranManagerSettings.b_LogError := FALSE;

  //set Hw off
  To_HwControl.SetHardwareOff( e_error := State );

  //produce Varan Manager Error
  if ( b_SetError & PreventManagerError = 0) then
    
    if b_PayloadSupport & _RTOSVersion >= VM_FULL_PAYLOAD_AVAILABLE  then
      MULTI_VARAN_iReportVaranError(diErrorNumber := ud_reason$DINT
                                  , hErrorNode    := vmNodeHandle);

    elsif pVaran & (pVaran^.udVersion >= 16#00001104 ) then
      VARAN_iSetManagerError( VaranManagerNr, ud_reason );
    end_if;
  end_if;

END_FUNCTION // VaranManager::LogError
